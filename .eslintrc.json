{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb-base", "plugin:prettier/recommended"],
    "plugins": ["prettier"],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "no-unused-vars": "off",
        "import/prefer-default-export": "off",
        "import/no-extraneous-dependencies": "off",
        "global-require": "off"
    },

    "globals": {
        "And": true,
        "cy": true,
        "describe": true,
        "export": true
    }
}
